      "build:prod": "npm run clean:public && npm run webpack -- --config config/webpack.prod.js --progress --profile --bail",
      "build:test": "npm run clean:public && npm run webpack -- --config config/webpack.test.js --progress --profile --bail",
      "lint": "eslint --config=./.eslintrc.js ./app",
